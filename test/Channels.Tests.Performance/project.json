{
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "allowUnsafe": true,
    "emitEntryPoint": true,
    "copyToOutput": [ "random.json.gz", "../../data/TestCert.pfx" ]
  },

  "publishOptions": {
    "includeFiles": [ "random.json.gz" ]
  },

  "dependencies": {
    "Channels": {
      "target": "project"
    },
    "Channels.Text.Primitives": {
      "target": "project"
    },
    "Channels.Networking.Sockets": {
      "target": "project"
    },
    "Channels.Networking.Libuv": {
      "target": "project"
    },
    "Channels.Networking.Windows.RIO": {
      "target": "project"
    },
    "Channels.File": {
      "target": "project"
    },
    "Channels.Compression": {
      "target": "project"
    },
    "Channels.Networking.TLS": {
      "target": "project" 
    },
    "Newtonsoft.Json": "9.0.1",
    "System.Text.Json": "0.1.0-*",
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0"
  },
  "frameworks": {
    "net46": {
      "imports": "dotnet",
      "dependencies": {
        "BenchmarkDotNet": "0.9.9",
        "BenchmarkDotNet.Diagnostics.Windows": "0.9.9"
      }
    }
  },

  "contentExclude": [ "BenchmarkDotNet.Artifacts**" ]
}